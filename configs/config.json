{
  "webpack": {
    "appMountNodeId": "app",
    "buildPath": "/dist"
  },
  "server": {
    "http": {
      "host": "http://localhost",
      "port": 4000
    },
    "paths": {
      "api": "/data",
      "er": "/er",
      "signIn": "/account/login",
      "signUp": "/account",
      "appRes": "/app"
    },
    "formFieldNames": {
      "signIn": {
        "username": "login",
        "password": "password"
      }
    },
    "authScheme": "Bearer"
  }
}
