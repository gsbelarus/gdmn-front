{
  "webpack": {
    "appMountNodeId": "app",
    "buildPath": "/dist"
  },
  "server": {
    "http": {
      "host": "http://localhost",
      "port": 4000
    },
    "paths": {
      "clientRoot": "/spa",
      "api": "/app/:uid/data",
      "er": "/app/:uid/er",
      "signIn": "/account/login",
      "signUp": "/account",
      "refreshAccessToken": "/account/refresh",
      "appRes": "/app",
      "backupRes": "/app/:uid/backup",
      "downloadBackup": "/app/:uid/backup/:backupUid/download",
      "restoreBackup": "/app/:uid/backup/:backupUid/restore"
    },
    "authScheme": "Bearer"
  }
}
